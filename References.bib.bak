@Article{Granger1980,
  Title                    = {An Introduction to Long Memory Time Series models and Fractional Differencing},
  Author                   = {Granger, C and Joyeux, R},
  Journal                  = {Journal of Time Series Analysis},
  Year                     = {1980},
  Number                   = {1},
  Pages                    = {15-29},
  Volume                   = {1},

  Abstract                 = {Abstract. The idea of fractional differencing is introduced in terms of the infinite filter that corresponds to the expansion of (1-B)^d. When the filter is applied to white noise, a class of time series is generated with distinctive properties, particularly in the very low frequencies and provides potentially useful long‐memory forecasting properties. Such models are shown to possibly arise from aggregation of independent components. Generation and estimation of these models are considered and applications on generated and real data presented.},
  Eprint                   = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1467-9892.1980.tb00297.x},
  Keywords                 = {Fractional differencing, long‐memory, integrated models}
}

