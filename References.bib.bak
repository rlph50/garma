@Article{Hosking1985,
  Title                    = {Fractional Differencing Modeling in Hydrology},
  Author                   = {Hosking, J},
  Journal                  = {Journal of the American Water Resources Association},
  Year                     = {1985},
  Number                   = {4},
  Pages                    = {677-682},
  Volume                   = {21},

  Abstract                 = {ABSTRACT: Fractional differencing is a tool for modeling time series which have long term dependence; i.e., series in which the correlation between distant observations, though small, is not negligible. Fractionally differenced ARIMA models are formed by permitting the differencing parameter d in the familiar Box‐Jenkins ARIMA(p, d, q) models to take nonintegral values; they permit the simultaneous modeling of the long term and short‐term behavior of an observed time series. This paper discusses the usefulness of fractional differencing to time‐series modeling, with emphasis on hydrologic applications. A methodology for fitting fractionally differenced ARIMA models is described, and examples are presented.},
  Eprint                   = {https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1752-1688.1985.tb05382.x},
  Keywords                 = {autoregressive moving–average process, fractional differencing, Hurst phenomenon, long‐term dependence, time series}
}

